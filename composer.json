{
    "name": "devster/frontmatter",
    "type": "library",
    "description": "A php `frontmatter` parser",
    "keywords": ["YAML", "Markdown", "parser", "frontmatter", "jekyll"],
    "homepage": "https://github.com/devster/frontmatter",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Perret",
            "email": "jeremy@devster.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/property-access": "~2.6"
    },
    "suggest": {
        "symfony/yaml": "Allows you to use native YAML parser",
        "erusev/parsedown-extra": "Allows you to use native Markdown parser",
        "seld/jsonlint": "Allows you to use native JSON parser"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "scrutinizer/ocular": "~1.1",
        "symfony/yaml": "~2.6",
        "erusev/parsedown-extra": "~0.7",
        "seld/jsonlint": "~1.3"
    },
    "autoload": {
        "psr-4": { "Devster\\Frontmatter\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
